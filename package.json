{
  "name": "panmetro-tizen-webtv",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "npm run build && npm-run-all --parallel serve proxy",
    "serve": "node scripts/serve-local.js",
    "proxy": "node scripts/proxy-server.js",
    "build": "webpack --mode production",
    "build:analyze": "ANALYZE=true webpack --mode production",
    "build:tizen": "npm run build && npm run copy-tizen-files && npm run verify:tizen",
    "copy-tizen-files": "node scripts/copy-tizen-files.js",
    "verify:tizen": "node scripts/verify-tizen-build.js",
    "get-tv-duid": "node scripts/get-tv-duid.js",
    "add-duid": "node scripts/add-duid-to-certificate.js",
    "package:tizen:dev": "npm run build:tizen && cd dist && zip -r ../PanMetroApp.wgt * && cd ..",
    "package:tizen:signed": "npm run build:tizen && tizen package -t wgt -s Panmetro -- dist && mv dist/PanMetroApp.wgt .",
    "package:tizen": "npm run package:tizen:dev",
    "deploy:cert": "node scripts/deploy-with-cert.js",
    "debug:tv": "node scripts/debug-tv-app.js",
    "install:tizen:dev": "npm run package:tizen:dev && tizen install -n PanMetroApp.wgt",
    "install:tizen:signed": "npm run package:tizen:signed && tizen install -n PanMetroApp.wgt",
    "install:tizen": "npm run install:tizen:dev",
    "run:tizen": "tizen run -p BtRWLGTTAm.PanMetroApp",
    "clean": "rm -rf dist node_modules/.cache *.wgt",
    "deploy:tizen:dev": "npm run build:tizen && npm run package:tizen:dev && npm run install:tizen:dev && npm run run:tizen",
    "deploy:tizen:signed": "npm run build:tizen && npm run package:tizen:signed && npm run install:tizen:signed && npm run run:tizen",
    "deploy:tizen": "npm run deploy:tizen:dev"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-transform-arrow-functions": "^7.23.3",
    "@babel/plugin-transform-block-scoping": "^7.23.4",
    "@babel/plugin-transform-classes": "^7.23.5",
    "@babel/plugin-transform-destructuring": "^7.23.3",
    "@babel/plugin-transform-parameters": "^7.23.3",
    "@babel/plugin-transform-runtime": "^7.23.4",
    "@babel/plugin-transform-spread": "^7.23.3",
    "@babel/plugin-transform-template-literals": "^7.23.3",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-react": "^7.23.3",
    "@babel/runtime": "^7.23.5",
    "babel-loader": "^8.3.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "compression-webpack-plugin": "^6.1.2",
    "css-loader": "^3.6.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.2",
    "mini-css-extract-plugin": "^0.12.0",
    "npm-run-all": "^4.1.5",
    "terser-webpack-plugin": "^4.2.3",
    "url-loader": "^4.1.1",
    "webpack": "^4.46.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^3.11.3"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.2.0",
    "@tanstack/react-query": "^5.59.19",
    "axios": "^1.11.0",
    "core-js": "^3.34.0",
    "feather-icons-react": "^0.9.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hot-toast": "^2.5.2",
    "react-qr-code": "^2.0.18",
    "react-router-dom": "^6.30.2",
    "regenerator-runtime": "^0.14.1",
    "shaka-player": "^4.15.8"
  }
}
